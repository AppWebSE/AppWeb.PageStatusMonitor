<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Mattias Andersson</Authors>
    <Company>AppWeb</Company>
    <Description>Package to enable monitoring the health status of your home page. Can send warnings and status updates through email or your own custom prefferred way when something is wrong.</Description>
    <RepositoryUrl>https://github.com/amattias/AppWeb.PageStatusMonitor</RepositoryUrl>
    <PackageProjectUrl>https://github.com/amattias/AppWeb.PageStatusMonitor</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/amattias/AppWeb.PageStatusMonitor/blob/master/LICENSE</PackageLicenseUrl>
    <PackageTags>Uptime Monitoring Page Website Health</PackageTags>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <Version>0.1.0</Version>
    <AssemblyVersion>0.0.1.0</AssemblyVersion>
    <FileVersion>0.0.1.0</FileVersion>
  </PropertyGroup>

</Project>
